{{#each this}}
  <img src='{{flag}}' alt='Country Flag' width='64px' />
  
  <h2>{{name}}</h2>
  <p>Capital: {{capital}}</p>
  <p>Population: {{population}}</p>
  <p>Languages:</p>
  <ul class='cc-det'>
    {{#each languages}}
      <li>{{name}}</li>
    {{/each}}
  </ul>
  <p>Currencies:</p>
  <ul class='cc-det'>
    {{#each currencies}}
      <li>{{symbol}} - {{name}} ({{code}})</li>
    {{/each}}
  </ul>
{{/each}}